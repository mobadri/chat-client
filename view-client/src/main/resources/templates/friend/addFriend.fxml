<?xml version="1.0" encoding="UTF-8"?>

<?import com.jfoenix.controls.JFXTextField?>
<?import javafx.scene.control.ListView?>
<?import javafx.scene.image.*?>
<?import javafx.scene.layout.*?>
<AnchorPane id="pane" prefHeight="400.0" prefWidth="329.0" stylesheets="@../../static/css/loginandsignup.css"
            xmlns="http://javafx.com/javafx/10.0.2-internal" xmlns:fx="http://javafx.com/fxml/1"
            fx:controller="com.chat.client.view.client.friend.AddFriend">
    <children>
        <JFXTextField fx:id="phoneNumberSearch" layoutX="59.0" layoutY="45.0" onKeyReleased="#search" prefHeight="25.0"
                      prefWidth="184.0" promptText="Phone Number" stylesheets="@../../static/css/loginandsignup.css"
                      unFocusColor="WHITE" AnchorPane.leftAnchor="8.0" AnchorPane.rightAnchor="32.0"/>
        <ListView fx:id="usersListView" layoutX="27.0" layoutY="70.0" prefHeight="200.0" prefWidth="299.0"
                  AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0"
                  AnchorPane.topAnchor="100.0"/>
        <AnchorPane layoutX="188.0" layoutY="14.0" prefHeight="25.0" prefWidth="127.0" AnchorPane.rightAnchor="14.0">
            <children>
                <ImageView fx:id="closeBtn" fitHeight="13.0" fitWidth="16.0" layoutX="100.0" layoutY="7.0"
                           onMouseClicked="#exit" pickOnBounds="true" preserveRatio="true"
                           AnchorPane.rightAnchor="14.0">
                    <image>
                        <Image url="@../../static/images/close.png"/>
                    </image>
                </ImageView>
                <ImageView fx:id="dashBtn" fitHeight="13.0" fitWidth="16.0" layoutX="57.0" layoutY="7.0"
                           onMouseClicked="#minimize" pickOnBounds="true" preserveRatio="true"
                           AnchorPane.rightAnchor="57.0">
                    <image>
                        <Image url="@../../static/images/dash.png"/>
                    </image>
                </ImageView>
                <ImageView fx:id="maximizeBtn" fitHeight="13.0" fitWidth="16.0" layoutX="80.0" layoutY="7.0"
                           onMouseClicked="#maximize" pickOnBounds="true" preserveRatio="true"
                           AnchorPane.rightAnchor="34.0">
                    <image>
                        <Image url="@../../static/images/minimize.png"/>
                    </image>
                </ImageView>
            </children>
        </AnchorPane>
        <ImageView fitHeight="25.0" fitWidth="36.0" layoutX="303.0" layoutY="45.0" pickOnBounds="true"
                   preserveRatio="true" AnchorPane.rightAnchor="1.0">
            <image>
                <Image url="@../../static/images/add.png"/>
            </image>
        </ImageView>

    </children>
</AnchorPane>
