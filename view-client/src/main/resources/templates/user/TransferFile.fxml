<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Label?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.text.Font?>

<AnchorPane xmlns:fx="http://javafx.com/fxml/1" prefHeight="55.0" prefWidth="667.0"
            xmlns="http://javafx.com/javafx/11.0.1"
            fx:controller="com.chat.client.view.client.chat.TransferFileController">
    <children>
        <HBox prefHeight="55.0" prefWidth="680.0">
            <children>
                <Label fx:id="nameOfFile" prefHeight="54.0" prefWidth="622.0" textFill="#828283">
                    <font>
                        <Font name="System Bold" size="12.0"/>
                    </font>
                </Label>
                <ImageView fitHeight="68.0" fitWidth="55.0" onMouseClicked="#handleSendFile" pickOnBounds="true"
                           preserveRatio="true">
                    <image>
                        <Image url="@../../static/images/sendFile.png"/>
                    </image>
                </ImageView>
            </children>
        </HBox>
    </children>
</AnchorPane>
