<?xml version="1.0" encoding="UTF-8"?>

<?import com.jfoenix.controls.JFXCheckBox?>
<?import com.jfoenix.controls.JFXPasswordField?>
<?import com.jfoenix.controls.JFXTextField?>
<?import javafx.geometry.Insets?>
<?import javafx.scene.control.*?>
<?import javafx.scene.image.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>
<?import java.net.URL?>
<BorderPane maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308"
            xmlns="http://javafx.com/javafx/10.0.2-internal" xmlns:fx="http://javafx.com/fxml/1"
            fx:controller="com.chat.client.view.client.login.LoginViewController">
    <center>
        <AnchorPane maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" prefHeight="620.0"
                    prefWidth="700.0" style="-fx-background-color: #fefefe;">


            <children>
                <ImageView fitHeight="620.0" fitWidth="700.0" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0"
                           AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
                    <image>
                        <Image url="@../../static/images/background.jpg"/>
                    </image>
                </ImageView>
                <AnchorPane layoutX="175.0" layoutY="71.0" prefHeight="448.0" prefWidth="350"
                            style="-fx-background-color: rgba(255,255,255,0.40); -fx-background-radius: 15px;">
                    <children>
                        <Label id="textbackground-black-bold" layoutY="69.0"
                               stylesheets="@../../static/css/loginandsignup.css" text="Login"
                               AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="32.0">
                            <font>
                                <Font name="System Bold" size="24.0"/>
                            </font>
                        </Label>
                        <Label id="textbackground-black" layoutX="32.0" layoutY="99.0" prefHeight="38.0"
                               prefWidth="214.0" text="Remeber to get up &amp; stretch once"
                               AnchorPane.leftAnchor="32.0" AnchorPane.rightAnchor="32.0">
                            <stylesheets>

                                <URL value="@../../static/css/loginandsignup.css"/>
                            </stylesheets>
                        </Label>
                        <Label id="textbackground-black" layoutX="32.0" layoutY="129.0"
                               stylesheets="@../../static/css/loginandsignup.css"
                               text=" in a while-your friends at chat" AnchorPane.leftAnchor="32.0"
                               AnchorPane.rightAnchor="32.0"/>
                        <JFXTextField fx:id="txtFieldLoginPhone" layoutX="12.0" layoutY="166.0" prefHeight="30.0"
                                      prefWidth="318.0" promptText="Phone" AnchorPane.leftAnchor="12.0"
                                      AnchorPane.rightAnchor="20.0">
                            <padding>
                                <Insets left="32.0" right="8.0"/>
                            </padding>
                        </JFXTextField>
                        <ImageView fitHeight="17.0" fitWidth="39.0" layoutX="310.0" layoutY="173.0" pickOnBounds="true"
                                   preserveRatio="true">
                            <image>
                                <Image url="@../../static/images/phone.png"/>
                            </image>
                        </ImageView>
                        <JFXPasswordField fx:id="txtFieldloginPassword" layoutX="12.0" layoutY="218.0" prefHeight="30.0"
                                          prefWidth="318.0" promptText="Password" AnchorPane.leftAnchor="12.0"
                                          AnchorPane.rightAnchor="20.0">
                            <padding>
                                <Insets left="32.0" right="8.0"/>
                            </padding>
                        </JFXPasswordField>
                        <ImageView fitHeight="17.0" fitWidth="27.0" layoutX="310.0" layoutY="225.0" pickOnBounds="true"
                                   preserveRatio="true">
                            <image>
                                <Image url="@../../static/images/password2.png"/>
                            </image>
                        </ImageView>
                        <Button id="buttons-blue" fx:id="btnLoginSignin" layoutX="60.0" layoutY="320.0"
                                mnemonicParsing="false" onAction="#onLogin" prefHeight="27.0" prefWidth="222.0"
                                stylesheets="@../../static/css/loginandsignup.css" text="Sign in" textFill="WHITE"
                                AnchorPane.leftAnchor="60.0" AnchorPane.rightAnchor="68.0">
                            <font>
                                <Font size="16.0"/>
                            </font>
                        </Button>
                        <Label id="textbackground-black" layoutY="396.0"
                               stylesheets="@../../static/css/loginandsignup.css" text="Don't have an account?"
                               textFill="#7182b5" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="137.0">
                            <font>
                                <Font size="14.0"/>
                            </font>
                        </Label>
                        <Hyperlink id="textbackground-blac" fx:id="lblloginSignuphere" layoutX="198.0" layoutY="392.0"
                                   onMouseClicked="#onSignUp" prefHeight="17.0" prefWidth="80.0"
                                   stylesheets="@../../static/css/loginandsignup.css" text="Sign up here"
                                   textFill="WHITE" AnchorPane.leftAnchor="198.0" AnchorPane.rightAnchor="-17.0">
                            <font>
                                <Font size="15.0"/>
                            </font>
                        </Hyperlink>
                        <JFXCheckBox fx:id="rememberMe" layoutX="42.0" layoutY="272.0" mnemonicParsing="false"
                                     prefHeight="21.0" prefWidth="129.0" text="Remember me" textFill="#354578"
                                     unCheckedColor="#354578">
                            <font>
                                <Font size="14.0"/>
                            </font>
                        </JFXCheckBox>
                        <Label fx:id="invalidPhone" layoutX="46.0" layoutY="196.0" AnchorPane.leftAnchor="46.0"/>
                        <Label fx:id="invalidPassword" layoutX="46.0" layoutY="255.0"/>
                    </children>

                </AnchorPane>
                <AnchorPane layoutX="606.0" prefHeight="40.0" prefWidth="94.0" AnchorPane.rightAnchor="0.0"
                            AnchorPane.topAnchor="0.0">
                    <children>
                        <ImageView fitHeight="21.0" fitWidth="34.0" layoutX="59.0" layoutY="15.0" onMouseClicked="#exit"
                                   pickOnBounds="true" preserveRatio="true">
                            <image>
                                <Image url="@../../static/images/close.png"/>
                            </image>
                        </ImageView>
                        <ImageView fitHeight="21.0" fitWidth="30.0" layoutX="-1.0" layoutY="17.0"
                                   onMouseClicked="#minimize" pickOnBounds="true" preserveRatio="true">
                            <image>
                                <Image url="@../../static/images/dash.png"/>
                            </image>
                        </ImageView>
                    </children>
                </AnchorPane>
            </children>
        </AnchorPane>
    </center>
</BorderPane>
